{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/menu/index",
      "style": {
        "navigationBarTitleText": "菜单"
      }
    },
    {
      "path": "pages/cart/index",
      "style": {
        "navigationBarTitleText": "购物车"
      }
    },
    {
      "path": "pages/profile/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/detail/index",
      "style": {
        "navigationBarTitleText": "商品详情"
      }
    },
    {
      "path": "pages/order/index",
      "style": {
        "navigationBarTitleText": "订单详情"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登陆页面"
      }
    },
		{
      "path": "pages/login/register",
      "style": {
        "navigationBarTitleText": "注册页面"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#848586",
    "selectedColor": "#0c34ba",
    "backgroundColor": "#fff",
    "borderStyle": "white",

    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/images/home.png",
        "selectedIconPath": "static/images/home_active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/menu/index",
        "iconPath": "static/images/menu.png",
        "selectedIconPath": "static/images/menu_active.png",
        "text": "菜单"
      },
      {
        "pagePath": "pages/cart/index",
        "iconPath": "static/images/shopbag.png",
        "selectedIconPath": "static/images/shopbag_active.png",
        "text": "购物袋"
      },
      {
        "pagePath": "pages/profile/index",
        "iconPath": "static/images/my.png",
        "selectedIconPath": "static/images/my_active.png",
        "text": "我的"
      }
    ]
  }
}
